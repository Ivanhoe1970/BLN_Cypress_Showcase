{
  "name": "bln-automation-showcase",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:protocol": "cypress run --spec 'cypress/e2e/protocols/**/*.cy.js'",
    "cypress:protocol-specific": "cypress run --spec 'cypress/e2e/local-testing/automated-basic-non-gas-alert-protocol/*.cy.js'",
    "cypress:debug": "cypress run --spec 'cypress/e2e/local-testing/automated-basic-non-gas-alert-protocol/protocol-dispatch-logic.cy.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:regression": "cypress run --spec 'cypress/e2e/regression-suite/**'",
    "test:regression:critical": "cypress run --spec 'cypress/e2e/regression-suite/critical-path/**'",
    "test:regression:components": "cypress run --spec 'cypress/e2e/regression-suite/component-tests/**'",
    "test:regression:integration": "cypress run --spec 'cypress/e2e/regression-suite/integration/**'",
    "test:regression:fast": "cypress run --spec 'cypress/e2e/regression-suite/**' --config video=false,screenshotOnRunFailure=true",
    "test:regression:open": "cypress open --spec 'cypress/e2e/regression-suite/**'"
  },
  "keywords": [],
