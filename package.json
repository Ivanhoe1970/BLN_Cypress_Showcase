  "description": "",
  "devDependencies": {
    "ajv": "^8.17.1",
    "cypress": "^14.0.2",
    "cypress-plugin-steps": "^1.2.0",
    "http-server": "^14.1.1",
    "wait-on": "^8.0.4"
  }
}
